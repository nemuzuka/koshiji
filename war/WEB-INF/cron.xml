<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
  <cron>
    <url>/_ah/sessioncleanup?clear</url>
    <description>Clean up sessions</description>
    <schedule>every 12 hours</schedule>
  </cron>
  <cron>
    <url>/admin/deletemessagetask</url>
    <description>delete older messages</description>
    <schedule>every day 00:00</schedule>
    <timezone>Asia/Tokyo</timezone>
    <target>deletemessage</target> 
  </cron>
</cronentries>
